{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/logs/IDP.log",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://ajf-elastic-01:9200",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv2"
        }
      }
    ]
  },
  "Version": "1.0.0.0",
  "ConnectionStrings": {
    "IdentityServer4": "Data Source=localhost;database=IdsIdentityServer4;User Id=ids;Password=ids;",
    "AspNetCoreIdentity": "Data Source=localhost;database=IdsAspNetCoreIdentity;User Id=ids;Password=ids;"
  },
  "AllowedHosts": "*",
  "UseAuthentication": true
}
