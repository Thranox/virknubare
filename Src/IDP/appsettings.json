{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=localhost;database=IDP-{Environment};User Id=ids;Password=ids;"
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/logs/Pol.IDP.log",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://ajf-elastic-01:9200",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv2"
        }
      }
    ]
  }
}