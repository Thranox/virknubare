{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/logs/Pol.APIOPEN.log",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://ajf-elastic-01:9200",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv2"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "PolConnection": "Data Source=localhost;database=Pol-{Environment};User Id=pol;Password=pol;"
  },
  "AllowedHosts": "*",
  "SubUsedWhenAuthenticationDisabled": "6E9D6247-8436-420B-A542-55B97B8B05E0"
}
